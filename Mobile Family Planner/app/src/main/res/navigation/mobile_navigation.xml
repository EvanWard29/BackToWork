<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_family">

    <fragment
        android:id="@+id/navigation_family"
        android:name="com.example.familyplanner.ui.family.FamilyFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_family" />

    <fragment
        android:id="@+id/navigation_chores"
        android:name="com.example.familyplanner.ui.chores.ChoresFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_admin_chores" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.familyplanner.ui.account.AccountFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_account" />
</navigation>